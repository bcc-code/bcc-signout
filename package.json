{
    "name": "bcc-signout",
    "version": "0.0.1",
    "description": "BCC service that enables Single Sign Out in Auth0",
    "main": "index.js",
    "scripts": {
        "build": "tsc",
        "start": "node -r dotenv/config ./dist/app.js",
        "dev": "nodemon -r dotenv/config --inspect=0.0.0.0:9229 ./src/app.ts",
        "debug": "export DEBUG=*,-controller:* && npm run dev",
        "docker": "export DEBUG=*,-controller:* && node -r dotenv/config -r ts-node/register --inspect=0.0.0.0:9229 ./src/app.ts",
        "test": "mocha -r ts-node/register \"test/**/*.ts\" --unhandled-rejections=strict --recursive --exit --timeout 5000",
        "test-dev": "mocha -r ts-node/register \"test/**/*.ts\" --unhandled-rejections=strict --recursive --exit --timeout 5000 --inspect-brk=0.0.0.0:9231",
        "test-debug": "export DEBUG=*,-controller:* && npm run test-dev",
        "pretty": "prettier --write \"./**/*.{js,jsx,json,ts}\"",
        "create-env": "printenv > .env"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bcc-code/bcc-signout.git"
    },
    "author": "Miko≈Çaj Kubista (mikolaj.kubista@bcc.no)",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/bcc-code/bcc-signout/issues"
    },
    "homepage": "https://github.com/bcc-code/bcc-signout#readme",
    "dependencies": {
        "axios": "^0.21.1",
        "cors": "^2.8.5",
        "debug": "^4.3.1",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-validator": "^6.12.0",
        "express-winston": "^4.1.0",
        "redis": "^3.1.2",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@types/chai": "^4.2.18",
        "@types/cors": "^2.8.10",
        "@types/debug": "^4.1.5",
        "@types/express": "^4.17.12",
        "@types/express-validator": "^3.0.0",
        "@types/mocha": "^8.2.2",
        "@types/redis": "^2.8.29",
        "chai": "^4.3.4",
        "eslint": "^7.28.0",
        "mocha": "^9.0.0",
        "nodemon": "^2.0.7",
        "prettier": "^2.3.1",
        "source-map-support": "^0.5.19",
        "ts-node": "^10.0.0",
        "typescript": "^4.3.2"
    }
}
